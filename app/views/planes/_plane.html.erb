<tbody>
  <tr>
    <td><%= plane.id %></td>
    <td><%= link_to plane.code, "#" %></td>
    <td><%= plane.status %></td>
    <td><%= t ".number" %> <%= plane.schedules.where(accept: true).last.location_id %></td>
    <td>
      <% if plane.schedules.last.processing %>
        <%= link_to "#" do %>
          <span class="label label-info fa-1x"><%=t(".processing")%></span>
        <% end %>
      <% end %>
    </td>
    <td>
      <% if plane.Ready? %>
        <%= link_to new_schedule_path locale, plane_id: plane.id, status: plane.status do %>
          <i class="fas fa-tools fa-2x" data-toggle="tooltip" data-placement="right" title=<%=t(".change_gara")%>></i>
        <% end %>
      <% else %>
        <%= link_to new_schedule_path locale, plane_id: plane.id, status: plane.status do %>
          <i class="fab fa-avianex fa-2x" data-toggle="tooltip" data-placement="right" title=<%=t(".change_ready")%>></i>
        <% end %>
      <% end %>
    </td>
  </tr>
</tbody>
